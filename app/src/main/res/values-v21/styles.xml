<resources>

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- API 14 theme customizations can go here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- <item name="android:windowEnterTransition">@android:transition/slide_bottom</item>
        <item name="android:windowExitAnimation">@android:transition/slide_left</item>-->
    <style name="Transition" parent="AppBaseTheme">
        <item name="android:windowAllowEnterTransitionOverlap">false</item>
        <item name="android:windowAllowReturnTransitionOverlap">false</item>
        <item name="android:windowActivityTransitions">true</item>
    </style>

  <!--  <style name="AppTheme.FitSystemWindown2" >
        <item name="android:windowTranslucentStatus" >true</item>
    </style>-->

</resources>
