<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--android:background="@color/half_gray"-->
<!-- android:background="@color/darker_blue"-->
    <com.github.jorgecastillo.FillableLoader
        android:id="@+id/fillableLoader"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_centerInParent="true"
        android:layout_gravity="center"
        app:fl_clippingTransform="waves"
        app:fl_fillColor="#88E91E63"
        app:fl_strokeColor="@color/black_2b"
        app:fl_fillDuration="10000"
        app:fl_originalHeight="512"
        app:fl_originalWidth="512"
        app:fl_strokeDrawingDuration="1000" />

</RelativeLayout>